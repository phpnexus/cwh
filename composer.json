{
    "name": "phpnexus/cwh",
    "homepage": "https://github.com/phpnexus/cwh",
    "type": "library",
    "description": "AWS CloudWatch Handler for Monolog library",
    "keywords": [
        "aws",
        "cloudwatch",
        "monolog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maksym Leonov",
            "email": "hi@maxleonov.pw",
            "homepage": "https://github.com/maxbanton"
        },
        {
            "name": "Mark",
            "homepage": "https://github.com/markinjapan"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "issues": "https://github.com/phpnexus/cwh/issues",
        "source": "https://github.com/phpnexus/cwh"
    },
    "require": {
        "php": ">=8.1",
        "monolog/monolog": "^3.0",
        "aws/aws-sdk-php": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "suggest": {
        "maxbanton/dd": "Minimalistic dump-and-die function for easy debugging"
    },
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "PhpNexus\\Cwh\\": "src"
        }
    },
    "minimum-stability": "stable"
}
